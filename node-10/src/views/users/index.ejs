<h1>Usuarios</h1>
<a href="/users/create" class="btn btn-sm btn-primary">Crear</a>

<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Correo</th>
      <th>Imagen</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% rows.forEach(user => { %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.email %></td>
      <td>
        <img
          src="uploads/<%= user.image || 'arbol.png' %>"
          width="100"
          alt="Foto perfil"
        />
      </td>
      <td></td>
    </tr>
    <% }) %>
  </tbody>
</table>
